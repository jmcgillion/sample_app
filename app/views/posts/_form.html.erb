<div class='well'>
  <%= form_for @obj do |f| %>
    <div class='control-group'>
      <%= f.label :title %>
      <%= f.text_area :title %>
    </div>
    <div class='control-group'>
      <%= f.label :body %>
      <%= f.text_area :body %>
    </div>
  <%= f.submit "New Post" %>
  <% end %>
</div>


