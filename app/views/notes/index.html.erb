<%= render 'shared/nav_bar_main' %>

<%= render 'nav_second' %>

<div class='container-fluid' style='padding-top: 5em'>
  <div class='row'>
    <div class='span0 col-lg-2 '>
      <div class="sidebar-nav">
            <div class="well">
            <ul class="nav nav-list"> 
              <li><%= link_to "Work", folder_path(:id => 12), class: 'btn btn-danger btn-small' %></li>
              <li><%= link_to "Play", folder_path(:id => 13), class: 'btn btn-success btn-small'%></li>
              <li><%= link_to "Well-Being", folder_path(:id => 14), class: 'btn btn-primary btn-small' %></li>
            </ul>
            </div>
        </div>
      </div>
      <div class='col-lg-8 col-lg-offset-1'>
        <table class='table table-striped table-hover' id='table_atts'>
        <% if logged_in? %>
          <tr>
          <% @notes.each do |note| %>
            <td><%= note.subject %></td>
            <td><%= note.body %></td>
            <td><%= display_datetime(note.created_at) %></td> 
          <% end %>
          </tr>
        <% end %>
        </table>
      </div>
    </div>
</div>










































